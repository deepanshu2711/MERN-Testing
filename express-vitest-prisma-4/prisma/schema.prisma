generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// we are moking it we dont need to connect a database connection

model Sum {
  id          Int @id   @default(autoincrement())
  a           Int
  b           Int
  result      Int
}
